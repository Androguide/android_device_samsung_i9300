#!/system/bin/sh

cp /system/GT-I9300.conf /data/ubuntu/etc/ubuntu-session.d/GT-I9300.conf
cp /system/Shell.qml /data/ubuntu/usr/share/qml-phone-shell/Shell.qml
chmod 755 /data/ubuntu/etc/ubuntu-session.d/GT-I9300.conf
chmod 755 /data/ubuntu/usr/share/qml-phone-shell/Shell.qml
chmod 755 /system/etc/init.d/99insmod

mount -o rw,remount /system
rm /system/etc/init.d/99ubuntu-session
rm /system/Shell.qml
rm /system/GT-I9300.conf
mount -o ro,remount /system